{
  "name": "nytimes-api-consume-demo-app",
  "version": "0.0.1",
  "description": "",
  "keywords": [
    "react",
    "redux",
    "redux-bundler",
    "nyt-api"
  ],
  "homepage": "",
  "bugs": "",
  "repository": "",
  "license": "MIT",
  "author": "Taras Gavrysh <ter4444@gmail.com>",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "precommit": "lint-staged",
    "dev": "webpack-dev-server --mode development --hot",
    "format": "prettier --write \"src/**/*.js\" && echo -e '\\033[0;32m'ðŸ’… Just use prettier'\\033[0m'",
    "lint": "eslint src",
    "start": "serve --single --cors dist",
    "test": "jest --coverage",
    "test:watch": "jest --watch --onlyChanged",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "dependencies": {
    "antd": "^3.9.2",
    "internal-nav-helper": "^1.0.2",
    "moment": "^2.22.2",
    "money-clip": "^2.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "redux-bundler": "^21.2.2",
    "redux-bundler-react": "^1.0.1",
    "styled-components": "^4.0.0-beta.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-async-to-generator": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/react": "^3.4.11",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-styled-components": "^1.7.1",
    "babel-runtime": "^6.26.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.5.0",
    "eslint-plugin-react": "^7.11.1",
    "html-loader": "^0.5.5",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.13",
    "ignore-loader": "^0.1.2",
    "jest": "^23.6.0",
    "jest-dom": "^2.0.0",
    "jest-styled-components": "^6.2.1",
    "lint-staged": "^7.2.2",
    "loki": "^0.13.0",
    "prettier": "^1.14.2",
    "react-hot-loader": "^4.3.6",
    "react-test-context-provider": "^2.2.0",
    "react-testing-library": "^5.1.0",
    "redux-testkit": "^1.0.6",
    "serve": "^10.0.0",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.3.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-pwa-manifest": "^3.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "git add"
    ]
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.docker",
        "width": 1366,
        "height": 768,
        "deviceScaleFactor": 1,
        "mobile": false,
        "fitWindow": false
      },
      "chrome.iphone7": {
        "target": "chrome.docker",
        "preset": "iPhone 7"
      }
    }
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^@components(.*)$": "<rootDir>/src/components$1",
      "^@bundles(.*)$": "<rootDir>/src/bundles$1",
      "^@common(.*)$": "<rootDir>/src/common$1",
      "^@screens(.*)$": "<rootDir>/src/screens$1",
      "^@tests(.*)$": "<rootDir>/src/tests$1"
    },
    "testPathIgnorePatterns": [
      "styled.js",
      "story.js"
    ],
    "setupFiles": [
      "<rootDir>/src/tests/setup.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/tests/extend-setup.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "text"
    ],
    "coverageDirectory": "coverage/",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "__tests__",
      "tests/",
      "styled.js",
      "story.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 30,
        "lines": 30,
        "statements": 30
      }
    }
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "engines": {
    "node": "9.10.0"
  }
}
